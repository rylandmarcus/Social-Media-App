
<h1>profiles show</h1>
<object data="<%= profile.coverPhoto %>" type="image/jpg">
    <img src="https://i.pinimg.com/1200x/ce/2c/16/ce2c167254400a9f2cf349019a5fcbfd.jpg" alt="alt">
</object>
<object data="<%= profile.profPic %>" type="image/jpg">
    <img src="https://cdn.pixabay.com/photo/2015/10/05/22/37/blank-profile-picture-973460_1280.png" alt="alt">
</object> 
<h2>
    <%= profile.firstName %> <%= profile.lastName %>
</h2>

<% if (myProf){ %>
    <a href="/posts/new">make a new post</a>
<% } %>

<a href="/profiles">Back to Profiles</a>
<a href="/posts">Back to Newsfeed</a>

<% posts.forEach(post=>{ %>
    <a href="/posts/<%= post._id %>">
        <p><%= post.body %></p>
    </a>
<% }) %>

